{
  "name": "@vozia/agent",
  "version": "0.0.2",
  "description": "Vozia AI Agent SDK - Chat, Voice, and Tools for Expo/React Native",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.js"
    },
    "./voice": {
      "types": "./dist/voice/index.d.ts",
      "import": "./dist/voice/index.mjs",
      "require": "./dist/voice/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": ">=1.21.0",
    "expo": ">=49.0.0",
    "expo-av": ">=13.0.0",
    "expo-haptics": ">=12.0.0",
    "expo-speech": ">=11.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.72.0",
    "react-native-reanimated": ">=3.0.0",
    "zustand": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "expo-av": {
      "optional": true
    },
    "expo-haptics": {
      "optional": true
    },
    "expo-speech": {
      "optional": true
    },
    "react-native-reanimated": {
      "optional": true
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@types/react": "^18.2.45",
    "react": "^18.2.0",
    "react-native": "^0.74.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "zustand": "^4.4.7"
  },
  "keywords": [
    "vozia",
    "ai",
    "agent",
    "expo",
    "react-native",
    "chat",
    "voice",
    "assistant",
    "sdk",
    "chatbot"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Renbo-Studios/vozia-agent-expo",
    "directory": "packages/agent"
  },
  "author": "Vozia",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
